#!/usr/bin/env python3

include: "common.snake"

rule all:
  input: path.tables/"done.create_tables.single_int_stat"

rule create_tables:
  input:
    socket=ancient(path.socket),
    schema=srcdir("scripts/dbschema/single_int_stat.py")
  output:
    touch(path.tables/"done.create_tables.single_int_stat")
  script:
    "scripts/db_create_tables.py"
